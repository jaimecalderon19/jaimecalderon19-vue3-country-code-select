{
  "name": "vue3-country-select",
  "version": "1.0.0",
  "description": "A Vue 3 component for country selection with TypeScript support",
  "main": "./dist/vue3-country-select.umd.cjs",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "author": "Jaime Calderon",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "country",
    "select",
    "country-select"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaimecalderon19/vue3-country-select"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "module": "./dist/vue3-country-select.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-country-select.js",
      "require": "./dist/vue3-country-select.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.8"
  }
}
